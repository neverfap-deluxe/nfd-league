{
  "name": "nfd-league",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/bar": "^0.56.2",
    "@nivo/line": "^0.56.2",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.14",
    "apollo-link-state": "^0.4.2",
    "graphql": "^14.2.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-iframe": "^1.8.0",
    "react-router-dom": "^5.0.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "3.0.0",
    "sitemap": "^2.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "react-scripts build && babel-node sitemap-generator.js && netlify deploy --prod",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sitemap": "babel-node sitemap-generator.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reactSnap": {
    "puppeteerArgs": [
      "--disable-setuid-sandbox",
      "--no-sandbox"
    ],
    "concurrency": 1,
    "inlineCss": false,
    "fixWebpackChunksIssue": false,
    "puppeteerExecutablePath": "/usr/bin/chromium-browser",
    "sitemap": true
  },
  "//": "puppeteerExecutablePath must be removed when building locally in watashi-client",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
